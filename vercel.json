{
    "rewrites": [
      {
        "source": "/comics/marvel/1",
        "destination": "https://getcomics.info/cat/marvel/page/1"
      },
      {
        "source": "/comics/dc/1",
        "destination": "https://getcomics.info/cat/dc/page/1"
      },
      {
        "source": "/comics/dc-week/1",
        "destination": "https://getcomics.info/tag/dc-week/page/1"
      },
      {
        "source": "/comics/vertigo/1",
        "destination": "https://getcomics.info/tag/vertigo/page/1"
      },
      {
        "source": "/comics/wildstorm/1",
        "destination": "https://getcomics.info/tag/wildstorm/page/:page"
      },
      {
        "source": "/comics/1",
        "destination": "https://getcomics.info/page/1"
      }
    ],
    "headers": [
      {
        "source": "/comics/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, X-Requested-With" }
        ]
      }
    ]
  }
  