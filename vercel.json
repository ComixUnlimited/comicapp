{
    "rewrites": [
      {
        "source": "/comics/marvel/page",
        "destination": "https://getcomics.info/cat/marvel/page"
      },
      {
        "source": "/comics/dc/page",
        "destination": "https://getcomics.info/cat/dc/page/"
      },
      {
        "source": "/comics/dc-week/1",
        "destination": "https://getcomics.info/tag/dc-week/page/"
      },
      {
        "source": "/comics/vertigo/1",
        "destination": "https://getcomics.info/tag/vertigo/page/"
      },
      {
        "source": "/comics/wildstorm/1",
        "destination": "https://getcomics.info/tag/wildstorm/page/:page"
      },
      {
        "source": "/comics/page",
        "destination": "https://getcomics.info/page"
      }
    ],
    "headers": [
      {
        "source": "/comics/:path*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, X-Requested-With" }
        ]
      }
    ]
  }
  